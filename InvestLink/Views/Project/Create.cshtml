                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>



    <script>
        let investorCount = 0;
    function addBootstrapInvestor() {
    investorCount++;
    const container = document.getElementById('investors-container');

    // إنشاء عنصر div للبطاقة
    const cardDiv = document.createElement('div');
    cardDiv.className = 'card mb-3 border shadow-sm investor-card-animation';
    cardDiv.id = investor-card-${investorCount};

    // محتوى البطاقة بتنسيق Bootstrap
    cardDiv.innerHTML = `
    <div class="card-body">
        <div class="d-flex justify-content-between align-items-center mb-3">
            <h6 class="card-title m-0"><span class="badge bg-primary rounded-pill me-2">${investorCount}</span> بيانات المستثمر</h6>
            ${investorCount > 1 ? <button type="button" class="btn btn-outline-danger btn-sm" onclick="removeInvestor(this)"><i class='bx bx-trash'></i> حذف</button> : ''}
        </div>

        <div class="row g-3">
            <div class="col-12 col-lg-6">
                <label class="form-label">أسم المستثمر</label>
                <input type="text" class="form-control" name="investor_name_${investorCount}">
            </div>
            <div class="col-12 col-lg-6">
                <label class="form-label">الجنسية</label>
                <input type="text" class="form-control" name="investor_nationality_${investorCount}">
            </div>
            <div class="col-12 col-lg-4">
                <label class="form-label">قيمة المساهمة</label>
                <input type="number" class="form-control" name="investor_value_${investorCount}">
            </div>
            <div class="col-12 col-lg-4">
                <label class="form-label">نوع العملة</label>
                <select class="form-select" name="investor_currency_${investorCount}">
                    <option value="LYD">دينار ليبي</option>
                    <option value="USD">دولار أمريكي</option>
                    <option value="EUR">يورو</option>
                </select>
            </div>
            <div class="col-12 col-lg-4">
                <label class="form-label">نسبة المساهمة (%)</label>
                <input type="number" class="form-control" name="investor_percent_${investorCount}">
            </div>
            <div class="col-12 col-lg-6">
                <label class="form-label">رقم الهاتف</label>
                <input type="text" class="form-control" name="investor_phone_${investorCount}">
            </div>
        </div>
    </div>
    `;

    container.appendChild(cardDiv);
    }

    function removeInvestor(btn) {
    btn.closest('.card').remove();
    }

    // إضافة مستثمر واحد تلقائياً عند تحميل الصفحة
    document.addEventListener('DOMContentLoaded', function() {
    addBootstrapInvestor();
    });
    </script>



